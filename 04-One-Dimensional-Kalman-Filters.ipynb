      "  7.949    2.003\t6.844\t  7.396   1.001\n",
      "  8.396    2.001\t9.847\t  9.122   1.000\n",
      " 10.122    2.000\t12.553\t 11.338   1.000\n",
      " 12.338    2.000\t16.273\t 14.305   1.000\n",
